{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}/extension"],
      "outFiles": [
        "${workspaceFolder}/extension/dist/**/*.js",
        "${workspaceFolder}/packages/core/dist/**/*.js"
      ],
      "preLaunchTask": "npm: build"
    }
  ]
}
